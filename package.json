{
  "name": "benefits-ufpls-ui-tests-pw",
  "engines": {
    "node": ">=20"
  },
  "version": "1.0.0",
  "description": "Benefits UFPLS UI Tests",
  "main": "index.js",
  "scripts": {
    "test:qa": "set NODE_ENV=qa && npx playwright test",
    "test:qa:ui": "set NODE_ENV=qa && npx playwright test --ui",
    "test:prod": "set NODE_ENV=prod && npx playwright test",
    "test:prod:ui": "set NODE_ENV=prod && npx playwright test --ui",
    "report": "allure generate --single-file ./allure-results -o ./allure-report --clean"
  },
  "author": "https://github.com/Lucaas27",
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.13.1",
    "allure-commandline": "^2.32.2",
    "allure-playwright": "^3.1.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "mongodb": "^6.13.0",
    "tedious": "^18.6.1",
    "winston": "^3.17.0"
  }
}
